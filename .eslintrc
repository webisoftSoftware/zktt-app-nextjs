{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-var": "error",
    "no-html-link-for-pages": "off"

  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      }
    }
  }
}
